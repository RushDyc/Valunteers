page {
  background-color: #f3f4f6;
}
.enroll_title {
  font-size: 35rpx;
  color: var(--themeColor);
  text-align: center;
  margin: 20rpx 0 ;
  padding: 10rpx 0 ;
  background-color: #fff;
  border: 1rpx solid #ccc;
  border-radius: 10rpx;
}
.enroll_content navigator {
  margin: 10rpx 0;
  background-color: #fff;
}
.enroll_content navigator .content_list {
  display: flex;
}
.enroll_content navigator .list_one {
  display: flex;
  flex-direction: column;
}
.enroll_content navigator .list_one .adress {
  margin-top: 12px;
  font-size: 12px;
  color: darkgray;
}
.enroll_content navigator .org {
  margin-top: 5px;
  font-size: 12px;
  color: darkgray;
}
.enroll_content navigator image {
  width: 28%;
}
